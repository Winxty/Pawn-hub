    while true do
        if isLooping then
            levelPart.Text = "Join Zefyr" -- Set to custom text when toggle is ON
        else
            levelPart.Text = originalLevel -- Revert to original text when toggle is OFF
        end
        wait(0.1) -- Prevent excessive resource usage
    end
end)



