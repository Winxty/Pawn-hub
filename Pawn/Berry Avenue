function LockKeySystem()

    local letters = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890!@#$%^&*()_+-=[]{}|;:',.<>?/ŒëŒíŒìŒîŒïŒñŒóŒòŒôŒöŒõŒúŒùŒûŒüŒ†Œ°Œ£Œ§Œ•Œ¶ŒßŒ®Œ©Œ±Œ≤Œ≥Œ¥ŒµŒ∂Œ∑Œ∏ŒπŒ∫ŒªŒºŒΩŒæŒøœÄœÅœÉœÑœÖœÜœáœàœâ–ê–ë–í–ì–î–ï–ñ–ó–ò–ô–ö–õ–ú–ù–û–ü–†–°–¢–£–§–•–¶–ß–®–©–´–≠–Æ–Ø–∞–±–≤–≥–¥–µ—ë–∂–∑–∏–π–∫–ª–º–Ω–æ–ø—Ä—Å—Ç—É—Ñ—Ö—Ü—á—à—â—ã—ç—é—èÿ£ÿ®ÿ¨ÿØ ŸáŸàÿ≤ ÿ≠ÿ∑Ÿä ŸÉŸÑŸÖŸÜ ÿ≥ÿπŸÅÿµ ŸÇÿ±ÿ≥ÿ™ ÿ´ÿÆÿ∞ ÿ∂ÿ∏ÿ∫◊ê◊ë◊í◊ì◊î◊ï◊ñ◊ó◊ò◊ô◊õ◊ú◊û◊†◊°◊¢◊§◊¶◊ß◊®◊©◊Å◊™‰Ω†Â•Ω‰∏ñÁïå‰∫∫‰ª¨ÊàëÁà±‰Ω†„Ç¢„Ç§„Ç¶„Ç®„Ç™„Ç´„Ç≠„ÇØ„Ç±„Ç≥„Çµ„Ç∑„Çπ„Çª„ÇΩ„Çø„ÉÅ„ÉÑ„ÉÜ„Éà„Éä„Éã„Éå„Éç„Éé„Éè„Éí„Éï„Éò„Éõ„Éû„Éü„É†„É°„É¢„É§„É¶„É®„É©„É™„É´„É¨„É≠„ÉØ„É≤„Çì„ÅÇ„ÅÑ„ÅÜ„Åà„Åä„Åã„Åç„Åè„Åë„Åì„Åï„Åó„Åô„Åõ„Åù„Åü„Å°„Å§„Å¶„Å®„Å™„Å´„Å¨„Å≠„ÅÆ„ÅØ„Å≤„Åµ„Å∏„Åª„Åæ„Åø„ÇÄ„ÇÅ„ÇÇ„ÇÑ„ÇÜ„Çà„Çâ„Çä„Çã„Çå„Çç„Çè„Çí„ÇìÍ∞ÄÎÇòÎã§ÎùºÎßàÎ∞îÏÇ¨ÏïÑÏûêÏ∞®Ïπ¥ÌÉÄÌååÌïò‡∏Å‡∏Ç‡∏Ñ‡∏Ü‡∏á‡∏à‡∏â‡∏ä‡∏ã‡∏å‡∏ç‡∏é‡∏è‡∏ê‡∏ë‡∏í‡∏ô‡∏ö‡∏õ‡∏ú‡∏ù‡∏û‡∏ü‡∏†‡∏°‡∏¢‡∏£‡∏•‡∏ß‡∏®‡∏©‡∏™‡∏´‡∏¨‡∏≠‡∏Æabcdefghijklmnopqrstuvwxyz√°√†·∫£√£·∫°·∫•·∫ß·∫©·∫´·∫°ƒÉ·∫Ø·∫±·∫≥·∫µ·∫∑√¢·∫•·∫ß·∫©·∫´·∫∑e√©√®·∫ª·∫Ω·∫π√™·∫ø·ªÅ·ªÉ·ªÖ·ªái√≠√¨·ªâƒ©·ªã√≤√≥·ªè√µ·ªç√¥·ªë·ªì·ªï·ªó·ªô∆°·ªõ·ªù·ªü·ª°·ª£u√∫√π·ªß≈©·ª•√¥·ªë·ªì·ªï·ªó·ªô∆°·ªõ·ªù·ªü·ª°·ª£c·∫£√†·∫£√£·∫°∆°·ªõ·ªù·ªü·ª°·ª£u√∫√π·ªß≈©·ª•√™·∫ø·ªÅ·ªÉ·ªÖ·ªái√≠√¨·ªâƒ©·ªãy√Ω·ª≥·ª∑·ªπ·ªµ"

    local length = math.random(10, 15)
    local randomString = ""

    -- Build the random string
    for i = 1, length do
        local randomIndex = math.random(1, #letters)
        randomString = randomString .. letters:sub(randomIndex, randomIndex)
    end

    -- Check if DEVUNLOCKCODE is correct, then set it to randomString
    if DEVUNLOCKCODE == "WeArePawniesForeal" then
        DEVUNLOCKCODE = randomString
    end

    -- Print the random string (for testing purposes)
    print("Random String: " .. randomString)
    

end








local Input = Tabs.Devt:AddInput("Input", {
    Title = "Dev Key",
    Description = "",
    Default = "None",
    Placeholder = "Placeholder",
    Numeric = false, -- Only allows numbers
    Finished = false, -- Only calls callback when you press enter
    Callback = function(Key)
        
        if Key == DEVUNLOCKCODE then
            print("Dev Unlocked")
            LockKeySystem()
            GetDev()
        else
            
        end

    end
})


